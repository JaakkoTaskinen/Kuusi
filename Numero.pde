public class Numero {

  public String tarkistaPaiva(int paiva) {
    switch(paiva) {
    case 1: 
      return "numero1.obj";
    case 2: 
      return "numero2.obj";
    case 3: 
      return "numero3.obj";
    case 4: 
      return "numero4.obj";
    case 5: 
      return "numero5.obj";
    case 6: 
      return "numero6.obj";
    case 7:  
      return "numero7.obj";
    default: 
      return "numero1.obj";
    }
  }
}

